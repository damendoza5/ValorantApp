{
  "expo": {
    "name": "Radianite",
    "slug": "ValorantApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "stretch",
      "backgroundColor": "#1B252D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B252D"
      }
    },
    "web": {
      "favicon": "./assets/radianite-logo.png"
    }
  }
}
